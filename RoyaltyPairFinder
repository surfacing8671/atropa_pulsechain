	}
]

async function main(){

const url = "https://api.dexscreener.com/latest/dex/tokens/0xC6F085DE4f388a48ce2942857455147A37870086"
const contract = new ethers.Contract("0xC6F085DE4f388a48ce2942857455147A37870086", royalAbi, provider)
const response = await axios.get(url)
let addresesPairs = []

await Promise.all(
response?.data?.pairs?.map(async (addresses) => {


    let divisor =  await contract.getbyaddress(addresses.pairAddress)
    let poolData = {
        liquidity: addresses.liquidity?.usd,
        pair: addresses.pairAddress,
        quoteToken: addresses.quoteToken.symbol,
        baseToken: addresses.baseToken.symbol,
        divisor: divisor
    }
    addresesPairs.push(poolData)
}))




console.log(addresesPairs)
}

main()

